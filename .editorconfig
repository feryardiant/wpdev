# editorconfig.org
root = true

[*]
end_of_line = lf
indent_style = space
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = space
indent_size = 4

[*.{js,yml,json,scss}]
indent_size = 2

[source/**.php]
indent_style = tab

[*.md]
trim_trailing_whitespace = false
